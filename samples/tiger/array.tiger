main let

    type goblinArray = array [5] of int;
    var goblins : goblinArray := 20;
    var number : int;
    var one: int := 1;

in begin

    goblins[3] := 11;
    printi(goblins[3] == 11);

    printi(goblins[4] == 20);

    number := goblins[2];
    printi(number == 20);

    number := goblins[one];
    printi(number == 20);

    /* 11 + 20 + 20 */
    printi(goblins[3] + goblins[4] + goblins[one] == 51);

end